mixin head(options)

  -
    const { descriptionValue, keywordsValue, title =' Toxin' } = options;

  head
    meta(charset="utf-8")
    meta(name="description" content=descriptionValue)
    meta(name="keywords" content=keywordsValue)
    meta(name="msapplication-TileColor" content="#da532c")
    meta(name="theme-color" content="#ffffff")
    meta(name="viewport" content="width=device-width,initial-scale=1")
    link(rel="apple-touch-icon" sizes="180x180" href=require('../favicons/apple-touch-icon.png'))
    link(rel="icon" type="image/png" sizes="42x42" href=require('../favicons/favicon-32x32.png'))
    link(rel="icon" type="image/png" sizes="16x16" href=require('../favicons/favicon-16x16.png'))
    link(rel="manifest" href=require('../favicons/site.webmanifest'))
    link(rel="mask-icon" href=require('../favicons/safari-pinned-tab.svg') color="#5bbad5")
    title=title
    